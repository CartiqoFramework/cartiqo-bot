{
	"name": "cartiqo",
	"version": "1.0.0",
	"description": "TypeScript discord.js bot template by Project Discord",
	"private": true,
	"workspaces": [
		"packages/*",
		"bots/*",
		"tools/*"
	],
	"homepage": "https://github.com/CartiqoFramework/cartiqo#readme",
	"bugs": {
		"url": "https://github.com/CartiqoFramework/cartiqo/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/CartiqoFramework/cartiqo.git"
	},
	"license": "MIT",
	"author": "CartiqoFramework",
	"scripts": {
		"build": "turbo run build --force",
		"dev": "turbo run dev --parallel",
		"lint": "turbo run lint",
		"format": "turbo run format",
		"Split": "________________________________________________________________",
		"start:cartiqo": "node ./bots/cartiqo/dist/index.js",
		"start:helper": "node ./bots/helper/dist/index.js",
		"start:processer": "node ./packages/processer/dist/index.js",
		"Split_2": "________________________________________________________________",
		"generate:bans": "npx prisma generate --config=prisma/bans/prisma.config.ts",
		"generate:cartiqo": "npx prisma generate --config=prisma/cartiqo/prisma.config.ts",
		"generate:global": "npx prisma generate --config=prisma/global/prisma.config.ts",
		"generate:all": "npm run generate:bans && npm run generate:cartiqo && npm run generate:global",
		"Split_3": "________________________________________________________________",
		"migrate:bans": "prisma migrate deploy --config=prisma/bans/prisma.config.ts",
		"migrate:cartiqo": "prisma migrate deploy --config=prisma/cartiqo/prisma.config.ts",
		"migrate:global": "prisma migrate deploy --config=prisma/global/prisma.config.ts",
		"migrate:all": "npm run migrate:bans && npm run migrate:cartiqo && npm run migrate:global",
		"Split_4": "________________________________________________________________",
		"migrate:bans:dev": "prisma migrate dev --config=prisma/bans/prisma.config.ts",
		"migrate:cartiqo:dev": "prisma migrate dev --config=prisma/cartiqo/prisma.config.ts",
		"migrate:global:dev": "prisma migrate dev --config=prisma/global/prisma.config.ts",
		"migrate:dev:all": "npm run migrate:bans:dev && npm run migrate:cartiqo:dev && npm run migrate:global:dev",
		"Split_5": "________________________________________________________________",
		"db:reset:bans": "npx prisma migrate reset --force --config=prisma/bans/prisma.config.ts",
		"db:reset:cartiqo": "npx prisma migrate reset --force --config=prisma/cartiqo/prisma.config.ts",
		"db:reset:global": "npx prisma migrate reset --force --config=prisma/global/prisma.config.ts",
		"db:reset:all": "npm run db:reset:bans && npm run db:reset:cartiqo && npm run db:reset:global",
		"db:studio": "npx prisma studio --config=prisma/global/prisma.config.ts",
		"Split_6": "________________________________________________________________",
		"setup:cartiqo": "npm run migrate:cartiqo && npm run generate:cartiqo",
		"setup:bans": "npm run migrate:bans && npm run generate:bans",
		"setup:global": "npm run migrate:global && npm run generate:global",
		"setup:all": "npm run migrate:all && npm run generate:all",
		"Split_7": "________________________________________________________________",
		"db:push:bans": "npx prisma db push --config=prisma/bans/prisma.config.ts",
		"db:push:cartiqo": "npx prisma db push --config=prisma/cartiqo/prisma.config.ts",
		"db:push:global": "npx prisma db push --config=prisma/global/prisma.config.ts",
		"db:push:all": "npm run db:push:bans && npm run db:push:cartiqo && npm run db:push:global",
		"Split_8": "________________________________________________________________",
		"check:bans": "npx prisma validate --config=prisma/bans/prisma.config.ts",
		"check:cartiqo": "npx prisma validate --config=prisma/cartiqo/prisma.config.ts",
		"check:global": "npx prisma validate --config=prisma/global/prisma.config.ts",
		"check:types": "tsc --noEmit"
	},
	"devDependencies": {
		"prettier": "^3.6.2",
		"prisma": "^7.0.0",
		"turbo": "^2.6.1"
	},
	"dependencies": {
		"@prisma/client": "^7.0.0"
	},
	"packageManager": "npm@11.6.0"
}
